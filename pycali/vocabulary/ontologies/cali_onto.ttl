@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix cali:  <http://cali.priloo.univ-nantes.fr/ontology#> .

cali:DeonticState  a  owl:Class , rdfs:Class  ;
          rdfs:label           "A deontic state" ;
          rdfs:comment         "The deontic state of an action (e.g., Permitted, Obliged, Prohibited, Recommended, etc.)" ;
          rdfs:subClassOf   owl:Thing ;
          rdfs:isDefinedBy     cali: .

cali:lessRestrictiveThan  a  owl:DatatypeProperty , rdf:Property ;
          rdfs:label        "less restrictive than" ;
          rdfs:comment      "An other thing that is less restrictive" ;
          rdfs:domain       owl:Thing ;
          rdfs:range        owl:Thing .

cali:Undefined  a  cali:DeonticState  ;
          rdfs:label        "Undefined state" ;
          rdfs:comment      "Stating that an action is in an Undefined state" .

cali:recommended  a  cali:DeonticState  ;
          rdfs:label        "Recommended state" ;
          rdfs:comment      "Stating that an action is recommended" .